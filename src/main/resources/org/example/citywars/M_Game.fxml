<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="root" prefHeight="1080" prefWidth="1920" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.citywars.M_Game">

    <ImageView fx:id="backGroundIm" fitHeight="1080.0" fitWidth="1920.0" />
    <Button alignment="CENTER" blendMode="SCREEN" contentDisplay="CENTER" layoutX="12.0" layoutY="12.0" mnemonicParsing="false" onAction="#GoToPauseButton" prefHeight="40.0" prefWidth="75.0" text="Pause" textFill="#650349">
        <font>
            <Font size="18.0" />
        </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
    </Button>
   <ImageView fitHeight="230.0" fitWidth="1820.0" layoutX="50.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../GameElements/timeline.png" />
      </image>
   </ImageView>
   <ImageView fx:id="timeLineWalker" fitHeight="222.0" fitWidth="58.0" layoutX="129.0" layoutY="74.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../GameElements/timelineWalker.png" />
      </image>
   </ImageView>
   <ImageView fitHeight="171.0" fitWidth="274.0" layoutX="20.0" layoutY="310.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../GameElements/lifeSpan.png" />
      </image>
   </ImageView>
    <ImageView fitHeight="171.0" fitWidth="274.0" layoutX="1620.0" layoutY="310.0" pickOnBounds="true" preserveRatio="true" style="-fx-scale-x: -1">
        <image>
            <Image url="@../../../GameElements/lifeSpan.png" />
        </image>
    </ImageView>
   <Button fx:id="bt" layoutX="617.0" layoutY="634.0" mnemonicParsing="false" text="Button" />

</Pane>
