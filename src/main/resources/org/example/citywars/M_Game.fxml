<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="1080" prefWidth="1920" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.citywars.M_Game">

    <ImageView fx:id="backGroundIm" fitHeight="1080.0" fitWidth="1920.0" />
    <Button alignment="CENTER" blendMode="SCREEN" contentDisplay="CENTER" layoutX="12.0" layoutY="12.0" mnemonicParsing="false" onAction="#GoToPauseButton" prefHeight="40.0" prefWidth="75.0" text="Pause" textFill="#650349">
        <font>
            <Font size="18.0" />
        </font>
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
    </Button>

</Pane>
