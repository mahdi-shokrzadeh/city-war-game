<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.citywars.M_LevelMap"
            prefHeight="1080" prefWidth="1920">
    <ImageView fx:id="backGroundIm" fitHeight="1080.0" fitWidth="1920.0" />
    <Button alignment="CENTER" blendMode="SCREEN" contentDisplay="CENTER" layoutX="1150.0" layoutY="780.0" mnemonicParsing="false" onAction="#GoToGamePlayModeButton" prefHeight="90.0" prefWidth="240.0" text="Back" textFill="#650349">
        <font>
            <Font size="30.0" />
        </font>
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor>
    </Button>
    <Button alignment="CENTER" blendMode="SCREEN" contentDisplay="CENTER" layoutX="1450.0" layoutY="780.0" mnemonicParsing="false" onAction="#GoToGameButton" prefHeight="90.0" prefWidth="240.0" text="Start Game" textFill="#650349">
        <font>
            <Font size="30.0" />
        </font>
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor>
    </Button>
</AnchorPane>
